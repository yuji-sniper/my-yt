{
  "name": "next-vibe-template",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": "24"
  },
  "scripts": {
    "dev": "next dev",
    "prebuild": "pnpm drizzle:migrate",
    "build": "next build",
    "start": "next start",
    "type:check": "tsc --noEmit",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format --write",
    "prepare": "husky",
    "drizzle:gen": "drizzle-kit generate --config src/backend/libs/drizzle/drizzle.config.ts",
    "drizzle:migrate": "drizzle-kit migrate --config src/backend/libs/drizzle/drizzle.config.ts",
    "drizzle:drop": "drizzle-kit drop --config src/backend/libs/drizzle/drizzle.config.ts"
  },
  "dependencies": {
    "@aws-sdk/client-scheduler": "^3.982.0",
    "@bprogress/next": "^3.2.12",
