    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.562.0",
    "mysql2": "^3.16.2",
    "next": "16.1.4",
    "next-intl": "^4.7.0",
    "next-themes": "^0.4.6",
    "pino": "^10.3.0",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-day-picker": "^9.13.2",
    "react-dom": "19.2.3",
